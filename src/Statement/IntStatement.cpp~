#include "IntStatement.hpp"
#include <Value/Value.hpp>
#include <sstream>


IntStatement::IntStatement(int intValue) {
	intValue_ = intValue;
}

std::string IntStatement::GenerateBytecode() {
	std::stringstream generated;
	std::string generated = "pushi ";
	generated += intValue_;
	generated += "\n";
	return generated;
}
