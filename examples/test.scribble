import("sys")

type Point := struct {
	x : int
	y : int
}

type Person := struct {
	Name : string
	Position : Point
}

func PrintPoint(p : Point) : void {
	sys.Write( sys.String(p->x) + " " + sys.String(p->y))
}

func PrintPerson(p : Person) : void {
	sys.Write( p->Name + "\n")
	PrintPoint( p->Position )
}

func main() : void {
	var p := Person { "blake", Point { 50, 40 } }
	PrintPerson( p )
}