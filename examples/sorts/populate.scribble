import("sys")

func PopulateWorstCase( list : array(int)) {

	for var i := 0; i < len(list); i++ do {
		list[i] := len(list) - i;
	}

}

func PopulateBestCase(list : array(int)) {

	for var i := 0; i < len(list); i++ do {
		list[i] := i;
	}

}

func PrintList(list : array(int)) {

	for var i := 0; i < len(list); i++ do {
		sys.Write( sys.String(list[i]) );
		sys.Write(" ");
	}
	
	sys.Write("\n");
}