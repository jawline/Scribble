func fib(var n : int) : int {

	var l1 : int := 0
	var l2 : int := 1
	
	if n = 0 {
		return 0
	}
	
	if n = 1 {
		return 1
	}
	
	n := n - 2
	
	for var i : int := 0; i < n; i := i + 1 {
		var fib : int := l1 + l2
		l2 := l1
		l1 := fib
	}

	return fib
}

func main() : void {
	var fibsTo : int := 7
	write("Calculating " + string(fibsTo) + " of fibs\n")
	var fibsResult : int := fib(7)
	write("Result: " + string(fibsResult) + "\n")
}